{
  "private": false,
  "name": "@attestprotocol/stellar-cli",
  "version": "1.3.0",
  "description": "Attest CLI for Stellar blockchain",
  "license": "MIT",
  "author": "Andrew Miracle <andrew@daccred.co>",
  "homepage": "https://github.com/daccred/attest.so#readme",
  "repository": "daccred/attest.so.git",
  "bugs": "https://github.com/daccred/attest.so/issues",
  "type": "commonjs",
  "main": "./bin/run.js",
  "bin": {
    "attest-stellar": "./bin/run.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "directories": {
    "bin": "bin",
    "lib": "src"
  },
  "scripts": {
    "build": "tsup-node",
    "build:watch": "tsup-node --watch",
    "clean": "rimraf dist",
    "commit": "cz",
    "commitlint": "commitlint --edit",
    "compile": "tsc",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "release": "semantic-release",
    "start": "ts-node ./bin/run.ts",
    "start:node": "node ./bin/run",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@semantic-release/github": "^11.0.1",
    "consola": "^3.2.3",
    "dotenv": "^16.4.5",
    "picocolors": "^1.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "22.15.18",
    "@types/prompts": "^2.4.9",
    "@types/signale": "^1.4.7",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "4.1.4",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.3",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "attestation",
    "blockchain",
    "cli",
    "stellar"
  ]
}
