{
  "name": "@attestprotocol/sdk",
  "description": "Meta-package SDK for interacting with Attest Protocol across multiple blockchains",
  "version": "2.1.0",
  "author": "daccred",
  "bugs": "https://github.com/daccred/attest.so/issues",
  "dependencies": {
    "@attestprotocol/core": "workspace:*",
    "@attestprotocol/solana-sdk": "workspace:*",
    "@attestprotocol/starknet-sdk": "workspace:*",
    "@attestprotocol/stellar-sdk": "workspace:*"
  },
  "devDependencies": {
    "@sebbo2002/semantic-release-jsr": "3.0.0",
    "@semantic-release/exec": "^7.0.3",
    "@types/bn.js": "^5.1.6",
    "@types/json-schema": "^7.0.15",
    "@types/urijs": "^1.19.25",
    "rimraf": "^6.0.1",
    "semantic-release-plugin-update-version-in-files": "^2.0.0",
    "ts-node": "^10.9.2",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1"
  },
  "directories": {
    "test": "test"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/daccred/attest.so#readme",
  "jsdelivr": "dist/umd/attestprotocol.js",
  "keywords": [],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "daccred/attest.so.git",
  "scripts": {
    "build": "pnpm clean && pnpm build:cjs && pnpm build:esm && pnpm build:types",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  }
}
