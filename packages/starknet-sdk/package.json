{
  "name": "@attestprotocol/starknet-sdk",
  "description": "Starknet implementation of the Attest Protocol SDK",
  "version": "2.0.0",
  "author": "daccred",
  "bugs": "https://github.com/daccred/attest.so/issues",
  "dependencies": {
    "@attestprotocol/core": "2.0.0",
    "starknet": "^6.6.6"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/daccred/attest.so#readme",
  "keywords": [
    "attestation",
    "blockchain",
    "cairo",
    "sdk",
    "starknet"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "daccred/attest.so.git",
  "scripts": {
    "build": "pnpm clean && pnpm build:cjs && pnpm build:esm && pnpm build:types",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo 'Tests skipped'",
    "test:coverage": "vitest --coverage"
  },
  "types": "dist/index.d.ts"
}
