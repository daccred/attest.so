{
  "name": "@attestprotocol/stellar-sdk",
  "description": "Stellar implementation of the Attest Protocol SDK",
  "version": "2.0.0",
  "author": "daccred",
  "bugs": "https://github.com/daccred/attest.so/issues",
  "dependencies": {
    "@attestprotocol/core": "2.0.0",
    "@attestprotocol/stellar-contracts": "2.0.0",
    "@noble/curves": "^2.0.0",
    "@noble/hashes": "^2.0.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "js-sha3": "^0.9.3",
    "ky": "^1.13.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./llm.txt": "./dist/llm.txt"
  },
  "files": [
    "dist",
    "src",
    "llm.txt"
  ],
  "homepage": "https://github.com/daccred/attest.so#readme",
  "keywords": [
    "attestation",
    "blockchain",
    "sdk",
    "soroban",
    "stellar"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "peerDependencies": {
    "@stellar/stellar-sdk": ">=14.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "daccred/attest.so.git",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:ts": "tsc --noEmit --pretty",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "types": "dist/index.d.ts"
}
