{
  "name": "@attestprotocol/core",
  "description": "Core abstractions and interfaces for Attest Protocol SDKs",
  "version": "1.7.6",
  "author": "daccred",
  "bugs": "https://github.com/daccred/attest.so/issues",
  "devDependencies": {
    "@types/node": "^24.2.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/daccred/attest.so#readme",
  "keywords": [
    "abstract",
    "attestation",
    "blockchain",
    "core",
    "sdk"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": "daccred/attest.so.git",
  "scripts": {
    "build": "pnpm clean && tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "types": "dist/index.d.ts"
}
